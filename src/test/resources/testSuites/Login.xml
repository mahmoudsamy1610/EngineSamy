<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite  name="Login"   preserve-order="true"  thread-count="20">

    <listeners>
        <listener class-name="Automation.engine.suiteWorks.SuiteDataGetterByRun"/>
        <listener class-name="Automation.engine.suiteWorks.HubBuilderByRun"/>
    </listeners>


    <test name = "test1">
        <parameter name="allure.results.directory" value="results/test1" />

        <classes>
            <class name="TestAutofox.gui.AdminLogin"/>
            <class name="TestAutofox.gui.AdminLogin2"/>
        </classes>
    </test>


    <test name = "test2">
        <parameter name="allure.results.directory" value="results/test2" />
        <classes>
            <class name="TestAutofox.gui.AdminLogin3"/>
            <class name="TestAutofox.gui.AdminLogin4"/>
        </classes>
    </test>

</suite>


